{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [],
    "plugins": [],
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "printWidth": 80,
          "trailingComma": "all",
          "semi": false,
          "jsxSingleQuote": true,
          "singleQuote": true,
          "useTabs": true,
          "endOfLine": "auto"
        }
      ]
    }
  }